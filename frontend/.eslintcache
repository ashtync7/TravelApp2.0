 or login\")\n\n})\n\nexport default actions;\n","/Users/ashtynczapansky/Desktop/Projects/TravelApp2.0/frontend/src/api/config.js",[],"/Users/ashtynczapansky/Desktop/Projects/TravelApp2.0/frontend/src/components/Cart.js",["119","120"],"import React, { useState } from 'react';\nimport customs from '../customs.json';\nimport ReactDOM from 'react-dom';\n\nfunction Cart(props) {\n\n    const [cart, setCart] = useState([])\n\n    const showCart = () => {\n        // let html = [];\n\n\n\n        return (\n            <h1>Cart</h1>\n        )\n\n    }\n\n    const setQuantity = (product, amount) => {\n        const newCart = [...cart];\n        newCart.find(\n            (item) => item.name === product.name\n        ).quantity = amount;\n        setCart(newCart);\n    };\n\n\n    return (\n        <div>\n            <h1>this is the cart</h1>\n            {showCart()}\n            <div className=\"products\">\n                {cart.map((product, idx) => (\n                    <div className=\"product\" key={idx}>\n                        <h3>{product.name}</h3>\n                        <h4>${product.cost}</h4>\n                        <input\n                            value={product.quantity}\n                            onChange={(e) =>\n                                setQuantity(\n                                    product,\n                                    parseInt(e.target.value)\n                                )\n                            }\n                        />\n                    </div>\n                ))}\n            </div>\n        </div>\n    );\n}\n\n\nexport default Cart;","/Users/ashtynczapansky/Desktop/Projects/TravelApp2.0/frontend/src/components/auth/GoogleAuth.js",[],"/Users/ashtynczapansky/Desktop/Projects/TravelApp2.0/frontend/src/components/auth/GoogleAuthLogin.js",[],"/Users/ashtynczapansky/Desktop/Projects/TravelApp2.0/frontend/src/components/auth/SignUp.js",[],"/Users/ashtynczapansky/Desktop/Projects/TravelApp2.0/frontend/src/compo